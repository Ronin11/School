package algos;

import java.util.ArrayList;

public class Recursive extends Algo {

	public Recursive(){
		
	}
	
	@Override
	public boolean win(int a, int b, int c) {
		if (a == 0 && b == 0 && c == 0) 
			return false;

		for(int i = 1; i < a; i++)
			if not Win(A-i, B, C) return True
		
		for(int i = 1; i < b; i++)
			if not Win(A,B-i,C) return True

		for(int i = 1; i < c; i++)
			if not Win(A,B,C-i) return True

		return false;
	}

}
