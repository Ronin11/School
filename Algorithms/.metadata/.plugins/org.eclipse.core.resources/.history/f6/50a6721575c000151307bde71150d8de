package algos;

public class Recursive extends Algo {

	public Recursive(){
		
	}
	
	@Override
	public boolean win(int a, int b, int c) {
		if (a == 0 && b == 0 && c == 0) 
			return false;

		for(int i = 1; i < a; i++)
			if(!win(a-i, b, c)) 
				return true;
		
		for(int i = 1; i < b; i++)
			if(!win(a, b-i, c)) 
				return true;

		for(int i = 1; i < c; i++)
			if(!win(a, b, c-i)) 
				return true;

		return false;
	}

}
