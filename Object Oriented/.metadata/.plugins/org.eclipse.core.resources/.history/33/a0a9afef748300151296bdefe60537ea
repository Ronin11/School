package io;

import java.io.File;
import java.io.FileNotFoundException;
import java.util.ArrayList;
import java.util.Scanner;

import sudoku.Board;

public class Import {
	File file;
	
	public Import(File file){
		this.file = file;
	}

	public Board nextBoard(File file){
		ArrayList<String> temp = new ArrayList<String>();
		temp.add("1234");
		temp.add("5678");
		temp.add("90AB");
		temp.add("CDEF");
		return new Board(4, temp);
	}
	
	private void nextBoardSize(){
		try {
			Scanner scan = new Scanner(file);
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		}
	}
}
