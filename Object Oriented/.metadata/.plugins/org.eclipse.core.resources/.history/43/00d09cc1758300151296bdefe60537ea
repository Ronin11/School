package main;

import java.util.ArrayList;

import io.Import;
import sudoku.Board;
import sudoku.Iterator;

public class Main {

	public static void main(String[] args) {
		Import importer = new Import(new File("test.txt"));
		Board b = new Board(4, importer.nextBoard());
		Iterator iter = b.getIterator();
		while(iter.hasNext())
			System.out.println(iter.next());
	}

}
