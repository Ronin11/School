		super(shape, canvas);
	}
	
	public void setPosition(int x, int y){this.x = x; this.y = y;}

	@Override
	public void doCommand() {
		
	}

	@Override
	public void undoCommand() {
		// TODO Auto-generated method stub
		
	}

}
